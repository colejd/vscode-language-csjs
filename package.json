{
    "name": "vscode-language-csjs",
    "displayName": "CSJS Syntax Highlighter",
    "description": "CSJS syntax highlighter for Visual Studio Code",
    "version": "1.0.0",
    "publisher": "colejd",
    "homepage": "https://github.com/colejd/vscode-language-csjs",
    "repository": {
        "type": "git",
        "url": "https://github.com/colejd/vscode-language-csjs"
    },
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "csjs",
            "aliases": ["CSJS", "csjs"],
            "extensions": [".csjs.js"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "csjs",
            "scopeName": "source.csjs",
            "path": "./syntaxes/csjs.tmLanguage.json"
        }]
    }
}